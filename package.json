{
  "name": "cryptoticker",
  "description": "Crypto stats chrome extension",
  "author": "kirqe <railsr@yahoo.com>",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e -g envify -t cssify src/main.js background.js -o dist/build.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js -t cssify | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "browserify-css": "^0.14.0",
    "browserify-shim": "^3.8.14",
    "bulma": "^0.6.1",
    "css-modulesify": "^0.28.0",
    "jquery": "^3.2.1",
    "sassify": "^4.0.1",
    "scssify": "^3.0.1",
    "underscore": "^1.8.3",
    "vue": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js",
    "jquery": "jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "jQuery"
  }
}
